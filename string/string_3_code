#include <iostream>
#include <string>
using namespace std;

int main() {
    string s = "abcabcbb";
    int n = s.size();

    int st = 0;              
    int pass = st + 1;       
    int length = 1;         
    int maxLen = 1;         

    while (st < n && pass < n) {
        if (s[st] == s[pass]) {
           
            st++;
            pass++;
        } else {
          
            length++;
            pass++;
            if (length > maxLen) {
                maxLen = length;
            }
        }
    }

    cout << "Longest leng " << maxLen << endl;
    return 0;
}
